{
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"clean": "rm -rf node_modules/.cache/babel-loader/* && rm -rf tmp",
		"deep-clean": "rm -rf node_modules && rm -rf tmp && yarn install && yarn start",
		"start": "concurrently 'yarn type-check' 'expo start'",
		"clean-start": "yarn start --reset-cache",
		"android": "concurrently 'yarn type-check' 'expo start --android'",
		"clean-android": "yarn android --reset-cache",
		"ios": "concurrently 'yarn type-check' 'expo start --ios'",
		"clean-ios": "yarn ios --reset-cache",
		"web": "concurrently 'yarn type-check' 'expo start --web'",
		"clean-web": "yarn web --reset-cache",
		"eject": "expo eject",
		"test": "jest --watchAll",
		"type-check": "tsc --watch"
	},
	"dependencies": {
		"@expo/vector-icons": "^12.0.0",
		"@reduxjs/toolkit": "^1.5.1",
		"axios": "^0.21.1",
		"date-fns": "^2.21.3",
		"expo": "~41.0.1",
		"expo-font": "~9.1.0",
		"expo-location": "~12.0.4",
		"expo-splash-screen": "~0.10.2",
		"expo-status-bar": "~1.0.4",
		"lodash": "^4.17.21",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
		"react-native-dotenv": "^2.5.5",
		"react-native-safe-area-context": "3.2.0",
		"react-native-web": "~0.13.12",
		"react-redux": "^7.2.4",
		"styled-components": "^5.3.0"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@types/lodash": "^4.14.170",
		"@types/react": "~16.9.35",
		"@types/react-native": "~0.63.2",
		"@types/react-redux": "^7.1.16",
		"@types/styled-components-react-native": "^5.1.1",
		"@typescript-eslint/eslint-plugin": "^4.24.0",
		"@typescript-eslint/parser": "^4.24.0",
		"babel-plugin-module-resolver": "^4.1.0",
		"concurrently": "^6.1.0",
		"eslint": "^7.27.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-react": "^7.23.2",
		"typescript": "~4.0.0"
	},
	"private": true
}
